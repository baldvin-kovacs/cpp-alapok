{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "-I",
        "${env:SFML_PATH}\\include",
        "/Zi",
        "/EHsc",
        "/nologo",
        "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
        "${file}",
        "nev_bekero.cc",
        "jatek.cc",
        "game_over.cc",
        "/link",
        "sfml-graphics.lib",
        "sfml-window.lib",
        "sfml-system.lib",
        "/LIBPATH:${env:SFML_PATH}\\lib"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": "build",
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}